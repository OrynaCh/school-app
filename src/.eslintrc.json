{
    "env": {
        "browser": true,
        "node": true
    },

    "plugins": ["import"],

    "extends": [
        "eslint:recommended",
        "airbnb/hooks"
    ],
    "globals": {

    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}